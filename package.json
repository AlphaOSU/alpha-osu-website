    "d3-scale": "^4.0.2",
    "dayjs": "^1.10.7",
    "dva": "2.6.0-beta.22",
    "history": "^5.0.1",
    "i18next": "^21.6.6",
    "lodash": "^4.17.21",
    "mathjax": "^3.2.2",
    "mathjax-full": "^3.2.2",
    "mathjax-react": "^1.0.0",
    "qs": "^6.10.3",
    "react": "^17.0.0",
