{
  "name": "alpha-osu-front",
  "version": "0.5.0-beta",
  "scripts": {
    "prepare": "npx husky install",
    "reinstall": "rm -rf node_modules && pnpm i",
    "dev": "vite",
    "build": "rm -rf dist && tsc && vite build",
